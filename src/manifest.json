{
  "manifest_version": 2,
  "name": "SuperMegaUltraD2RU",
  "version": "1.0.5",
  "content_scripts": [
    {
      "matches": [
        "https://dota2.ru/*"
      ],
      "run_at": "document_start",
      "match_about_blank": true,
      "js": [
        "js/index.js"
      ]
    }
  ],
  "web_accessible_resources": ["css/*", "js/*"],
  "icons": {
    "16": "icons/16.png"
  }
}